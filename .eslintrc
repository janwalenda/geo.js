{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": { 
        "no-console": 2, // Remember, this means error!
        "sort-vars": "warn",
        "space-before-blocks": "error",
        "complexity": "warn",
        "yoda": "warn",
        "linebreak-style": "warn",
    }
  }